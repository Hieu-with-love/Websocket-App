# You following this template to create your own .env file for the Identity Service.
# Good luck!

# Database Configuration
DB_PASSWORD=your_database_password
DB_USERNAME=your_database_username

# JWT Configuration
JWT_SIGNER_KEY=your_jwt_signer_key_here

# OAuth2 Google Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Application URLs
OAUTH_REDIRECT_URL=http://localhost:3000/auth/callback
PROFILE_SERVICE_URL=http://localhost:8081/profile

# Database URL (contains sensitive connection info)
DB_URL=jdbc:mysql://localhost:3306/identity_service?useSSL=false&allowPublicKeyRetrieval=true&serverTimezone=UTC
